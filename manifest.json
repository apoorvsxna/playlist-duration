{
    "manifest_version": 3,
    "name": "Playlist Duration",
    "description": "Quickly find the total duration of any YouTube playlist",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon.png"
    },
    "host_permissions": [
      "https://youtube.googleapis.com/"
    ],
    "icons": {
      "128": "icons/icon.png"
    }
  }
  